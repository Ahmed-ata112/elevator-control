add wave -position end  sim:/resolver_comb/ups
add wave -position insertpoint  \
sim:/resolver_comb/buttons \
sim:/resolver_comb/clk \
sim:/resolver_comb/downs \
sim:/resolver_comb/highest_dest \
sim:/resolver_comb/lowest_dest \
sim:/resolver_comb/N \
sim:/resolver_comb/no_button_is_pressed \
sim:/resolver_comb/none_is_pressed \
sim:/resolver_comb/ups
force -freeze sim:/resolver_comb/clk 1 0, 0 {5 ns} -r 10
force buttons 0FF
force ups 030
force downs 030
run
force buttons 0F0
run
force buttons 0F8
run
force buttons 0F7
run
force ups 000
run
force downs 000
run
force buttons 000
run
force buttons FFF
run
run
run
run
force buttons F0F
run
run
run
force buttons FFF
run
force downs 00F
run

